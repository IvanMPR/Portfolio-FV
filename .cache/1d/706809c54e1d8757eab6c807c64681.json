queries\r\n  @include respond(phone) {\r\n    &-right {\r\n      align-items: center;\r\n      padding-bottom: 0.5rem;\r\n\r\n      &__ul {\r\n        display: none;\r\n      }\r\n    }\r\n\r\n    .hamburger-menu {\r\n      margin-right: 2rem;\r\n\r\n      .uil-apps {\r\n        cursor: pointer;\r\n        font-size: var(--font-l);\r\n        display: block;\r\n      }\r\n    }\r\n  }\r\n  @include respond(tab-p) {\r\n    &-right {\r\n      align-items: center;\r\n      padding-bottom: 0.5rem;\r\n\r\n      &__ul {\r\n        display: none;\r\n      }\r\n    }\r\n\r\n    .hamburger-menu {\r\n      margin-right: 2rem;\r\n      .uil-apps {\r\n        cursor: pointer;\r\n        font-size: var(--font-l);\r\n        display: block;\r\n      }\r\n    }\r\n\r\n    .uil-moon,\r\n    .uil-sun {\r\n      font-size: var(--font-l);\r\n    }\r\n  }\r\n}\r\n\r\n.scroll-bar {\r\n  height: 5px;\r\n  width: 0;\r\n  margin: 0 3rem;\r\n  background-color: var(--color-bg-offset);\r\n}\r\n","scss\\components\\_section.scss":"@use '../abstracts/' as *;\r\n\r\nsection {\r\n  min-height: 95vh;\r\n  width: 100vw;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"f627fe027dd6a76bac62e87f459a2dba","cacheData":{"env":{}}}