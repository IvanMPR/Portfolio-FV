252526;\r\n  --color-text: #ffffff;\r\n  --color-text-offset: #818a91;\r\n  --color-border: #2d2d2d;\r\n  --color-primary: #ff5277;\r\n  --color-primary-offset: #ff2957;\r\n  --color-secondary: #43a9a3;\r\n}\r\n","scss\\base\\_base.scss":"@use '../abstracts/' as *;\r\n\r\n*,\r\n*::after,\r\n*::before {\r\n  padding: 0;\r\n  margin: 0;\r\n  box-sizing: inherit;\r\n}\r\n\r\nbody {\r\n  width: 100vw;\r\n  height: 100%;\r\n  background-color: var(--color-bg);\r\n  box-sizing: border-box;\r\n  padding: 2rem;\r\n  transition: background-color 0.2s;\r\n  // position: relative;\r\n}\r\n\r\na {\r\n  text-decoration: none;\r\n  display: inline-block;\r\n}\r\n\r\nhtml {\r\n  font-size: 62.5%;\r\n  scroll-behavior: smooth;\r\n  // media queries\r\n  @include respond(phone) {\r\n    & {\r\n      font-size: 45%;\r\n    }\r\n  }\r\n\r\n  @include respond(tab-p) {\r\n    & {\r\n      font-size: 50%;\r\n    }\r\n  }\r\n\r\n  @include respond(tab-l) {\r\n    & {\r\n      font-size: 60%;\r\n    }\r\n  }\r\n\r\n  @include respond(big-desk) {\r\n    & {\r\n      font-size: 65%;\r\n    }\r\n  }\r\n}\r\n","scss\\base\\_utilities.scss":"@use '../abstracts/' as *;\r\n\r\n.active {\r\n  border-bottom: 0.5rem solid var(--color-primary) !important;\r\n}\r\n\r\n.sticky {\r\n  position: fixed;\r\n  padding: 1rem;\r\n  max-width: 120rem;\r\n  margin: auto;\r\n  top: 2rem;\r\n}\r\n\r\n.container {\r\n  padding: 1rem;\r\n  max-width: 120rem;\r\n  margin: auto;\r\n\r\n  @include respond(phone) {\r\n    & {\r\n      padding: 1rem 1.5rem;\r\n    }\r\n  }\r\n  @include respond(tab-p) {\r\n    & {\r\n      padding: 1rem 2rem;\r\n    }\r\n  }\r\n}\r\n","scss\\components\\_header.scss":"@use '../abstracts/' as *;\r\n@use '../base/' as *;\r\n\r\n.nav {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  margin: 0 3rem;\r\n  padding: 0 3rem 2rem;\r\n  // border-bottom: 1px solid var(--color-primary);\r\n\r\n  &-left {\r\n    display: flex;\r\n    align-items: center;\r\n\